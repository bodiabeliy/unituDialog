{
  "name": "my-qwik-empty-starter",
  "description": "App with Routing built-in ready to create your app",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "trustedDependencies": [
    "sharp"
  ],
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr --host localhost",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --port=40956",
    "start": "vite --mode ssr --port=40956",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.2.17",
    "@builder.io/qwik-city": "^1.2.17",
    "@types/eslint": "8.44.1",
    "@types/node": "^20.4.5",
    "@typescript-eslint/eslint-plugin": "6.2.0",
    "@typescript-eslint/parser": "6.2.0",
    "autoprefixer": "^10.4.14",
    "eslint": "8.45.0",
    "eslint-plugin-qwik": "^1.2.17",
    "postcss": "8.4.31",
    "prettier": "3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "tailwindcss": "3.3.5",
    "typescript": "5.1.6",
    "undici": "5.27.2",
    "vite": "4.5.0",
    "vite-tsconfig-paths": "4.2.1"
  },
  "dependencies": {
    "sharp": "^0.32.6",
    "webm-to-wav-converter": "^1.1.0",
    "websocket": "^1.0.34"
  }
}
